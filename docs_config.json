{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
		"include": [ "index.js", "timeWindow.js" ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
		"default" : {
			"staticFiles": {
				"include": [
					"./static"
				]
			}
		}
    },
	"opts" : {
		"destination": "./docs/",
		"readme": "intro.md"
	}
}
